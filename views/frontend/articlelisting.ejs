          <a href="/post/<%= listingNews.news_category%>/<%= listingNews.id%>/<%= replacesting %>">
        
    <div class="articles-head"><% var newTitle = new String(listingNews.news_title) 
                            if(newTitle.length > 70 ) {  var Slicetext = newTitle.slice(0,70);
                          %> <%= Slicetext+'...' %>  <%} else {%> <%= listingNews.news_title%> <%}%>
</div>
   
            
    
    <p class="articles-des">
        
        
        
         <% var htmlText=listingNews.news_des;
                                            var noHTML = /(<([^>]+)>)/ig; 
                                            var clean = htmlText.replace(noHTML, '');
                                             
